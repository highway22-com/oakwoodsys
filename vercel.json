{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/oaw/browser",
  "framework": null,
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/home-content",
      "destination": "/api/home-content"
    },
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    }
  }
}